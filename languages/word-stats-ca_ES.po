#, php-format
msgid "One %1$sregular expression%2$s per line, without slashes."
msgstr "Una %1$sexpressiò regular%2$s per llínea, sense barres."

#: word-stats.php:553
msgid "Example: ^apples?$ = good, /^apples?$/ = bad."
msgstr "Exemple: ^torrats?$ = ben, /^torrats?$/ = mal."

#: word-stats.php:561
msgid "Writing basic regular expressions:"
msgstr "Escribint expressions regulars bàsiques:"

#: word-stats.php:562
msgid "^ matches the beggining of the word."
msgstr "^ coincideix amb el principi de la paraula."

#: word-stats.php:562
msgid "Example: \"^where\" matches \"wherever\" but not \"anywhere\"."
msgstr "Exemple: \"^vol\" coincideix amb \"volta\", però no amb \"qualsevol\"."

#: word-stats.php:563
msgid "$ matches the end of the word."
msgstr "$ coincidex el final de la paraula."

#: word-stats.php:563
msgid "Example: \"where$\" matches \"anywhere\" but not \"wherever\"."
msgstr "Exemple: \"vol$\" coincideix amb \"qualsevol\", però no amb \"volta\"."

#: word-stats.php:564
msgid "^keyword$ matches only the whole keyword."
msgstr "^paraula$ coincideix només amb la paraula completa."

#: word-stats.php:564
msgid "Example: \"^where$\" matches \"where\" but not \"anywhere\" or \"wherever\"."
msgstr "Exemple: \"^vol$\" coincideix amb \"vol\", però no amb \"qualsevol\" o \"volta\"."

#: word-stats.php:565
msgid "? matches the previous character zero or one time."
msgstr "? coincideix amb el caràcter anterior zero o una vegada."

#: word-stats.php:565
msgid "Example: \"^apples?$\" matches \"apple\" and \"apples\"."
msgstr "Exemple: \"^torrats?$\" coincideix amb \"torrat\" i \"torrats\"."

#: word-stats.php:566
msgid "* matches the previous character zero or more times."
msgstr "* coincideix amb el caràcter anterior zero o més vegades."

#: word-stats.php:566
msgid "Example: \"^10*$\" matches \"1\", \"10\", \"1000000\", etc."
msgstr "Exemple: \"^10*$\" coincideix amb \"1\", \"10\", \"100000\", etc."

#: word-stats.php:567
msgid "+ matches the previous character one or more times."
msgstr "+ coincideix amb el caràcter anterior una o més vegades."

#: word-stats.php:567
msgid "Example: \"^10+$\" matches \"10\", \"1000000\", etc., but not \"1\"."
msgstr "Exemple: \"^10+$\" coincideix \"10\", \"1000000\", etc., però no amb \"1\"."

#: word-stats.php:568
msgid "[] matches any of the characters between the brackets."
msgstr "[] coincideix amb qualsevol dels caràcters entre els claudàtors."

#: word-stats.php:568
msgid "Example: \"^take[ns]?$\" matches \"take\", \"taken\" and \"takes\"."
msgstr "Exemple: \"^torne[ns]?$\" coincideix amb \"torne\", \"tornes\", \"tornen\"."

