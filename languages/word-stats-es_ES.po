#: view-settings.php:81
#, php-format
msgid "One %1$sregular expression%2$s per line, without slashes."
msgstr "Una %1$sexpresión regular%2$s por línea, sin barras."

#: view-settings.php:82
msgid "Example: ^apples?$ = good, /^apples?$/ = bad."
msgstr "Ejemplo: ^apples?$ = bien, /^apples?$/ = mal."

#: view-settings.php:90
msgid "Writing basic regular expressions:"
msgstr "Escribiendo expresiones regulares básicas:"

#: view-settings.php:91
msgid "^ matches the beggining of the word."
msgstr "^ coincide con el principio de la palabra."

#: view-settings.php:91
msgid "Example: \"^where\" matches \"wherever\" but not \"anywhere\"."
msgstr "Ejemplo: \"^donde\" coincide con \"dondequiera\" pero no \"adonde\"."

#: view-settings.php:92
msgid "$ matches the end of the word."
msgstr "$ coincide con el final de la palabra."

#: view-settings.php:92
msgid "Example: \"where$\" matches \"anywhere\" but not \"wherever\"."
msgstr "Ejemplo: \"donde$\" coincide con \"adonde\" pero no con \"dondequiera\"."

#: view-settings.php:93
msgid "^keyword$ matches only the whole keyword."
msgstr "^palabra$ coincide sólo con la palabra completa."

#: view-settings.php:93
msgid "Example: \"^where$\" matches \"where\" but not \"anywhere\" or \"wherever\"."
msgstr "Ejemplo: \"^donde$\" coincide con \"donde\" pero no con \"dondequiera\" o \"adonde\"."

#: view-settings.php:94
msgid "? matches the previous character zero or one time."
msgstr "? coincide con el carácter anterior cero o una vez."

#: view-settings.php:94
msgid "Example: \"^apples?$\" matches \"apple\" and \"apples\"."
msgstr "Ejemplo: \"^manzanas?$\" coincide con \"manzana\" y \"manzanas\"."

#: view-settings.php:95
msgid "* matches the previous character zero or more times."
msgstr "* coincide con el carácter anterior cero o más veces."

#: view-settings.php:95
msgid "Example: \"^10*$\" matches \"1\", \"10\", \"1000000\", etc."
msgstr "Ejemplo: \"^10*$\" coincide con \"1\", \"10\", \"1000000\", etc."

#: view-settings.php:96
msgid "+ matches the previous character one or more times."
msgstr "+ coincide con el carácter anterior una o más veces."

#: view-settings.php:96
msgid "Example: \"^10+$\" matches \"10\", \"1000000\", etc., but not \"1\"."
msgstr "Ejemplo: \"^10+$\" coincide con \"10\", \"1000000\", etc., pero no con \"1\"."

#: view-settings.php:97
msgid "[] matches any of the characters between the brackets."
msgstr "[] coincide con cualquiera de los caracteres entre los corchetes."

#: view-settings.php:97
msgid "Example: \"^take[ns]?$\" matches \"take\", \"taken\" and \"takes\"."
msgstr "Ejemplo: \"^toma[ns]?$\" coincide con \"toma\", \"tomas\" y \"toman\"."

